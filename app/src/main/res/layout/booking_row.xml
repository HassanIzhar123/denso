<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="15dp"
    android:layout_margin="10dp"
    app:cardBackgroundColor="#F5F6F9"
    app:cardCornerRadius="10dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <RelativeLayout
            android:id="@+id/orderrel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/ordertext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Order"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/ordercharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/ordertext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/ordervaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/ordercharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/daterel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/orderrel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/datetext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Date"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/datecharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/datetext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/datevaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/datecharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/timerel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/daterel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/timetext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Time"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/timecharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/timetext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/timevaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/timecharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/bookservicerel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/timerel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/bookservicetext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Book Service"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/bookservicecharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/bookservicetext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/bookservicevaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/bookservicecharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/makerel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bookservicerel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/maketext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Make"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/makecharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/maketext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/makevaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/makecharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/yearrel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/makerel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/yeartext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Year"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/yearcharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/yeartext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/yearvaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/yearcharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tranmissionrel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/yearrel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/tranmissiontext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Transmission"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tranmissioncharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/tranmissiontext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tranmissionvaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/tranmissioncharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/branchrel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tranmissionrel"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

            <TextView
                android:id="@+id/branchtext"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/helvetica"
                android:text="Branch"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/branchcharactertext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/branchtext"
                android:text=":"
                android:textColor="#262A33"
                android:textSize="15sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/branchvaluetext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/branchcharactertext"
                android:fontFamily="@font/helvetica"
                android:text="Value"
                android:textColor="#262A33"
                android:textSize="15sp" />

        </RelativeLayout>

    </RelativeLayout>


</androidx.cardview.widget.CardView>